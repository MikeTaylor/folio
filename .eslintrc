{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-multiple-empty-lines": "off",
    "import/extensions": "off",
    "max-len": "off",
    "arrow-parens": "off",
    "no-unused-vars": [ "warn", { "destructuredArrayIgnorePattern": "^_" } ]
  }
}
